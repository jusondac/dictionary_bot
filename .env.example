# Example environment file
# Copy this to .env and fill in your actual values

# Discord Bot Token - Get this from Discord Developer Portal
DISCORD_TOKEN=your_discord_bot_token_here

# Dictionary API Key - Get this from Dictionary API (optional for some APIs)
DICTIONARY_API_KEY=your_dictionary_api_key_here

# WordsAPI Key - Alternative API for more features (RapidAPI)
WORDS_API_KEY=your_words_api_key_here

# Bot Configuration
BOT_PREFIX=!
